{
    "builds": [
{
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    },
{
        "src": "Python/wsgi.py",
        "use": "@vercel/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "python" }
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Python/wsgi.py"
        }
    ]
}